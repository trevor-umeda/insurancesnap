<% form_for @snapshot, :html => { :multipart => true } do |f| %>
    <%= render 'shared/error_messages', :object => f.object %>
      <div class = "field">
        <%= f.label :title %>
        <%= f.text_field :title %>
      </div>
      <div class = "field">
        <%= f.file_field :photo %>
      </div>
    <p> <%= f.submit "Submit" %></p>
<% end %>
